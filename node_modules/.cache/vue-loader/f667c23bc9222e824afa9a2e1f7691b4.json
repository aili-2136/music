\n\t\t\t\t\t\t00:{{item<=9?'0'+item:item}}\r\n\t\t\t\t\t</div>\r\n\t\t \t\t</div>\r\n\t\t\t\t<p id=\"cw\">{{prompt}}</p>\r\n\t\t\t\t<button @click=\"wc\">完成</button>\r\n\t\t </div>\r\n\t\t \r\n\t\t<!-- 注册 -->\r\n\t\t<div id=\"a\" v-show='iszuce'>\r\n\t\t\t<a href=\"#\" id=\"fanhui\" @click=\"goback\"><返回登录</a>\r\n\t\t\t\t<div class=\"fab fa-pinterest\" id=\"tu\"></div>\r\n\t\t\t\t<div id=\"phone\">\r\n\t\t\t\t\t<p class=\"iconfont icon-shouji\"></p>\r\n\t\t\t\t\t<input type=\"tel\" v-model=\"zuce.phone\" placeholder=\"请输入手机号\" @focus=\"isnone\">\r\n\t\t\t\t</div>\r\n\t\t\t\t<div id=\"password\">\r\n\t\t\t\t\t<p class=\"iconfont icon-mima\"></p>\r\n\t\t\t\t\t<input type=\"password\" v-model=\"zuce.password\" placeholder=\"设置登录密码\" @focus=\"isnone\">\r\n\t\t\t\t</div>\r\n\t\t\t\t<p id=\"cw\">{{prompt}}</p>\r\n\t\t\t\t<button @click=\"goyz\">下一步</button>\r\n\t\t</div>\r\n\t\t<!-- 注册验证 -->\r\n\t\t<div id=\"a\" v-show=\"isyz\">\r\n\t\t\t<a href=\"#\" id=\"fanhui\" @click=\"goback\"><返回登录</a>\r\n\t\t\t\t<div class=\"fab fa-pinterest\" id=\"tu\"></div>\r\n\t\t\t\t<p style=\"font-size: 14px;margin-top: 15px;\">为了安全，我们会为你的手机发送短信校验码</p>\r\n\t\t\t\t<div id=\"yz\">\r\n\t\t\t\t\t<div id=\"yza\">\r\n\t\t\t\t\t\t<p class=\"iconfont icon-yanzhengyanzhengma\"></p>\r\n\t\t\t\t\t\t<input type=\"text\" id=\"te\" placeholder=\"填写验证码\" v-model=\"zuce.verify\" @focus=\"isnone\">\r\n\t\t\t\t\t</div>\r\n\t\t\t\t\t<div id=\"yzb\">\r\n\t\t\t\t\t\t00:{{item<=9?'0'+item:item}}\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t\t<p id=\"cw\">{{prompt}}</p>\r\n\t\t\t\t<button @click=\"gonc\">下一步</button>\t\t\r\n\t\t</div>\r\n\t\t<!-- 设置昵称 -->\r\n\t\t<div id=\"a\" v-show=\"isnc\">\r\n\t\t\t\t<div class=\"iconfont icon-gaoxiaoquwei\" id=\"tu\" style=\"font-size: 50px;\"></div>\r\n\t\t\t\t<p style=\"font-size: 14px;margin-top: -10px;\">取一个昵称，让大家记住您</p>\r\n\t\t\t\t<div id=\"nc\">\r\n\t\t\t\t\t<p class=\"iconfont icon-denglu2\"></p>\r\n\t\t\t\t\t<input type=\"text\" placeholder=\"昵称不少于4个英文或2个汉字\" v-model=\"zuce.name\" @focus=\"isnone\">\r\n\t\t\t\t</div>\r\n\t\t\t\t<p id=\"cw\">{{prompt}}</p>\r\n\t\t\t\t<button @click=\"go\">开启云音乐</button>\t\t\r\n\t\t</div>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\timport {\r\n\t\tcheckout\r\n\t} from '../.././general/regular.js'\r\n\timport {\r\n\t\tgetLoginCellphone,\r\n\t\tgetRegisterCellphone,\r\n\t\tgetCaptchaSent,\r\n\t\tgetCaptchaVerify,\r\n\t\tgetRegisterAnonimous,\r\n\t\tgetLoginRefresh\r\n\t} from '../.././plugins/register.js'\r\n\texport default {\r\n\t\tname:'regsiter',\r\n\t\tdata(){\r\n\t\t\treturn {\r\n\t\t\t\t//登录\r\n\t\t\t\tregsiter:{\r\n\t\t\t\t\tphone:'',\t\t\r\n\t\t\t\t\tpassword:''\r\n\t\t\t\t},\r\n\t\t\t\t//注册\r\n\t\t\t\tzuce:{\r\n\t\t\t\t\tphone:'',\r\n\t\t\t\t\tpassword:'',\r\n\t\t\t\t\tverify:'',\r\n\t\t\t\t\tname:''\r\n\t\t\t\t},\r\n\t\t\t\t//修改\r\n\t\t\t\txiugai:{\r\n\t\t\t\t\tphone:'',\r\n\t\t\t\t\tpassword:'',\r\n\t\t\t\t\tverify:''\r\n\t\t\t\t},\r\n\t\t\t\t//验证\r\n\t\t\t\tyanzen:{\r\n\t\t\t\t\tphone:'',\r\n\t\t\t\t\tverify:''\r\n\t\t\t\t},\r\n\t\t\t\titem:'',\r\n\t\t\t\tisitem:60,\r\n\t\t\t\ttext:'',\r\n\t\t\t\thqyzm:'获取验证码',\r\n\t\t\t\tprompt:'',\t\t//提示\r\n\t\t\t\t\r\n\t\t\t\t\r\n\t\t\t}\r\n\t\t},\r\n\t\tmethods:{\r\n\t\t\t//叉叉\r\n\t\t\tshowx(){\r\n\t\t\t\tthis.$store.state.isshow=false\r\n\t\t\t\tthis.$store.dispatch('zuce',false)\r\n\t\t\t\tthis.$store.dispatch('yz',false)\r\n\t\t\t\tthis.$store.dispatch('nc',false)\r\n\t\t\t\tthis.$store.dispatch('wjmm',false)\r\n\t\t\t\tthis.$store.dispatch('yzmdl',false)\r\n\t\t\t\tthis.$store.dispatch('ggyz',false)\r\n\t\t\t\tthis.$store.dispatch('yzm',false)\r\n\t\t\t\tthis.$store.dispatch('ishqyzm',true)\r\n\t\t\t\tthis.prompt=''\r\n\t\t\t\tthis.regsiter.password=''\r\n\t\t\t\tthis.regsiter.phone=''\r\n\t\t\t\tclearInterval(this.text)\r\n\t\t\t},\r\n\t\t\t//忘记密码\r\n\t\t\twjmm(){\r\n\t\t\t\tthis.prompt=''\r\n\t\t\t\tthis.$store.dispatch('wjmm',true)\r\n\t\t\t\tthis.$store.dispatch('denglu',false)\r\n\t\t\t\tthis.xiugai.phone=''\r\n\t\t\t\tthis.xiugai.pas