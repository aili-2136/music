"prev, pager, next\"\r\n\t\t\t\t:total=\"1000\">\r\n\t\t\t</el-pagination>\r\n\t\t</div>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\timport {\r\n\t\tgetPlaylistCatlist,\r\n\t\tgetPlaylistHot,\r\n\t\tgetTopPlaylist,\r\n\t\tgetPlaylistHighqualityTags,\r\n\t\tgetTopPlaylistHighquality\r\n\t} from '../../../.././plugins/song.js'\r\n\texport default {\r\n\t\tname: 'songlist',\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\tdome: '全部歌单',\r\n\t\t\t\tsub: [], //具体分类\r\n\t\t\t\thotarr: [], //热门分类\r\n\t\t\t\tplaylist: [], //歌单\r\n\t\t\t\thighqualitytags: [], //精品歌单分类\r\n\t\t\t\thighqualit: {\r\n\t\t\t\t\tdata:[],\r\n\t\t\t\t\tname:'',\r\n\t\t\t\t\timg:''\r\n\t\t\t\t}, //精品歌单\r\n\t\t\t\tonejp: {\r\n\t\t\t\t\timg: '',\r\n\t\t\t\t\tname: ''\r\n\t\t\t\t},\r\n\t\t\t\tlanguage: [], //语种\r\n\t\t\t\tstyle: [], //风格\r\n\t\t\t\tscene: [], //场景\r\n\t\t\t\temotion: [], //情感\r\n\t\t\t\ttheme: [], //主题\r\n\t\t\t\tdisp: false, //判断隐藏导航是否出现\r\n\t\t\t\tmt: {},\r\n\t\t\t\tlist: new Map(), //用来存放分页请求来的数据\r\n\t\t\t\tcurrentPage: 1, //初始页\r\n\t\t\t\tpageParams: { //页码，每页几条\r\n\t\t\t\t\tpage: 1,\r\n\t\t\t\t\tpagesize: 100\r\n\t\t\t\t},\r\n\t\t\t\tcurPage: 0 //当前页数\t\t\t\t\r\n\t\t\t}\r\n\t\t},\r\n\t\tmounted() {\r\n\t\t\tthis.$store.dispatch('dhtz',2)\r\n\t\t\t//获取分类和具体分类\r\n\t\t\tgetPlaylistCatlist().then(res => {\r\n\t\t\t\tthis.sub = res.data.sub\r\n\t\t\t\t//将具体分类划分开\r\n\t\t\t\tconst a = () => {\r\n\t\t\t\t\tfor (let i = 0; i < this.sub.length; i++) {\r\n\t\t\t\t\t\tif (this.sub[i].category === 0) {\r\n\t\t\t\t\t\t\tthis.language.push(this.sub[i].name)\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\tif (this.sub[i].category === 1) {\r\n\t\t\t\t\t\t\tthis.style.push(this.sub[i].name)\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\tif (this.sub[i].category === 2) {\r\n\t\t\t\t\t\t\tthis.scene.push(this.sub[i].name)\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\tif (this.sub[i].category === 3) {\r\n\t\t\t\t\t\t\tthis.emotion.push(this.sub[i].name)\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t\tif (this.sub[i].category === 4) {\r\n\t\t\t\t\t\t\tthis.theme.push(this.sub[i].name)\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t\ta()\r\n\r\n\t\t\t})\r\n\r\n\r\n\t\t\t//获取热门分类\r\n\t\t\tgetPlaylistHot().then(res => {\r\n\t\t\t\tthis.hotarr = res.data.tags\r\n\t\t\t})\r\n\r\n\r\n\t\t\t//获取歌单\r\n\t\t\tgetTopPlaylist({\r\n\t\t\t\tlimit: 100\r\n\t\t\t}).then(res => {\r\n\t\t\t\tthis.playlist = res.data.playlists\r\n\t\t\t})\r\n\r\n\t\t\t//获取精品歌单标签列表\r\n\t\t\tgetPlaylistHighqualityTags().then(res => {\r\n\t\t\t\tthis.highqualitytags = res.data.tags\r\n\t\t\t})\r\n\r\n\t\t\t//获取精品歌单\r\n\t\t\tgetTopPlaylistHighquality().then(res => {\r\n\t\t\t\tthis.highqualit.data = res.data.playlists\r\n\t\t\t\tthis.highqualit.img=res.data.playlists[0].coverImgUrl\r\n\t\t\t\tthis.highqualit.name=res.data.playlists[0].name\r\n\t\t\t})\r\n\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\t//根据标签获取歌单\r\n\t\t\tshow(name) {\r\n\t\t\t\t\r\n\t\t\t\tgetTopPlaylist({\r\n\t\t\t\t\tlimit: 100,\r\n\t\t\t\t\tcat: name\r\n\t\t\t\t}).then(res => {\r\n\t\t\t\t\tthis.playlist = res.data.playlists\r\n\t\t\t\t})\r\n\r\n\t\t\t\t//根据标签获取精品歌单\t\r\n\t\t\t\tgetTopPlaylistHighquality({\r\n\t\t\t\t\tlimit: 100,\r\n\t\t\t\t\tcat: name\r\n\t\t\t\t}).then(res => {\r\n\t\t\t\t\tthis.highqualit.data = res.data.playlists\r\n\t\t\t\t\tthis.highqualit.img=res.data.playlists[0].coverImgUrl\r\n\t\t\t\t\tthis.highqualit.name=res.data.playlists[0].name\r\n\t\t\t\t})\r\n\r\n\t\t\t\tthis.dome = name\r\n\t\t\t},\r\n\t\t\tnone() {\r\n\t\t\t\tthis.disp = !this.disp\r\n\t\t\t\tif (this.disp === true) {\r\n\t\t\t\t\tthis.mt = {\r\n\t\t\t\t\t\tmarginTop: '-679px'\r\n\t\t\t\t\t}\r\n\t\t\t\t} else {\r\n\t\t\t\t\tthis.mt = {}\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\tshownone(name) {\r\n\t\t\t\tthis.disp = !this.disp\r\n\t\t\t\tif (this.disp === true) {\r\n\t\t\t\t\tthis.mt = {\r\n\t\t\t\t\t\tmarginTop: '-679px'\r\n\t\t\t\t\t}\r\n\t\t\t\t} else {\r\n\t\t\t\t\tthis.mt = {}\r\n\t\t\t\t}\r\n\t\t\t\tgetTopPlaylist({\r\n\t\t\t\t\tlimit: 100,\r\n\t\t\t\t\tcat: name\r\n\t\t\t\t}).then(res => {\r\n\t\t\t\t\tthis.play